<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ilib.Controller.ControllerBookAdmin">
   <children>
      <TableView fx:id="BookTable" layoutX="2.0" layoutY="51.0" prefHeight="542.0" prefWidth="659.0">
        <columns>
          <TableColumn fx:id="bookIDCol" prefWidth="122.0" text="bookID" />
          <TableColumn fx:id="titleCol" minWidth="0.0" prefWidth="154.0" text="title" />
            <TableColumn fx:id="authorCol" prefWidth="130.0" text="Author" />
            <TableColumn fx:id="priceCol" prefWidth="126.0" text="Price" />
            <TableColumn fx:id="quanityCol" prefWidth="126.0" text="Quantity" />
        </columns>
      </TableView>
      <VBox fx:id="searchVbox" layoutX="690.0" layoutY="330.0" prefHeight="256.0" prefWidth="161.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="AddButton" mnemonicParsing="false" onMouseClicked="#AddButtonClick" prefHeight="51.0" prefWidth="110.0" text="Add" textFill="#2cab25">
                     <font>
                        <Font size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../assets/green-add-button-12023.png" />
                     </image>
                     <HBox.margin>
                        <Insets top="15.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="EditButton" mnemonicParsing="false" onMouseClicked="#EditButtonClick" prefHeight="51.0" prefWidth="110.0" text="Edit" textFill="#882ebf">
                     <font>
                        <Font size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../assets/pencil-2317.png" />
                     </image>
                     <HBox.margin>
                        <Insets top="15.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <HBox prefHeight="54.0" prefWidth="197.0">
               <children>
                  <Button fx:id="DeleteButton" mnemonicParsing="false" onMouseClicked="#DeleteButtonClick" prefHeight="51.0" prefWidth="110.0" text="Delete" textFill="#d3401b">
                     <font>
                        <Font size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <ImageView fitHeight="30.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../assets/trash-can-10417.png" />
                     </image>
                     <HBox.margin>
                        <Insets top="15.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
         </children>
      </VBox>
      <TextField fx:id="searchText" layoutX="690.0" layoutY="51.0" onKeyPressed="#searchEnter" onKeyReleased="#searchKeyBoard" prefHeight="54.0" prefWidth="161.0" promptText="Search" />
      <ScrollPane fx:id="searchScroll" layoutX="690.0" layoutY="105.0" prefHeight="200.0" prefWidth="161.0" visible="false">
         <content>
            <VBox fx:id="searchVbox" prefHeight="219.0" prefWidth="162.0" />
         </content>
      </ScrollPane>
      <Button fx:id="RefreshButton" mnemonicParsing="false" onMouseClicked="#RefreshButtonClick" prefHeight="51.0" prefWidth="110.0" text="Refresh" textFill="#17614e">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
